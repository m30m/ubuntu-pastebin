{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  
  "content_scripts": [
    {
      "matches": ["*://paste.ubuntu.com/*"],
      "js": ["jquery.js", "scr.js"]
    }
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}